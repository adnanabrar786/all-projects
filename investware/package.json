{
  "name": "investware-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next dev": "sst bind next dev",
    "start:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "eslint": "eslint",
    "check-types": "tsc",
    "check-format": "prettier --check .",
    "lint": "npm run eslint && npm run check-types",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "format": "prettier --write .",
    "prisma:generate:local": "dotenv -e .env.local -- npx prisma generate",
    "prisma:pull:local": "dotenv -e .env.local -- npx prisma db pull && dotenv -e .env.local -- npx prisma generate"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.14",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@mui/styled-engine-sc": "^6.0.0-alpha.18",
    "@mui/x-data-grid": "^7.15.0",
    "@mui/x-date-pickers": "^7.14.0",
    "@prisma/client": "^5.18.0",
    "@tanstack/react-query": "^5.51.24",
    "aws-amplify": "5.3.11",
    "axios": "^1.7.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "formik": "^2.4.6",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "react-toastify": "^10.0.5",
    "styled-components": "^6.1.12",
    "use-debounce": "^10.0.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "aws-cdk-lib": "2.124.0",
    "constructs": "10.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "sst": "2.40.3",
    "standard-version": "^9.5.0",
    "typescript": "^5"
  }
}
