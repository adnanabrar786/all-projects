{
  "name": "weddingdaytimeline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "test": "nx test",
    "prepare": "husky install",
    "check-format": "prettier --check .",
    "check-types": "tsc",
    "eslint": "eslint",
    "lint": "npm run eslint && npm run check-types",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "format": "prettier --write .",
    "prisma:generate:local": "dotenv -e .env.local -- npx prisma generate",
    "prisma:pull:local": "dotenv -e .env.local -- npx prisma db pull && dotenv -e .env.local -- npx prisma generate",
    "prisma:pull:dev": "dotenv -e .env.dev -- npx prisma db pull && dotenv -e .env.dev -- npx prisma generate",
    "prisma:pull:test": "dotenv -e .env.test -- npx prisma db pull && dotenv -e .env.test -- npx prisma generate",
    "prisma:pull:prod:v3": "dotenv -e .env.prod.v3 -- npx prisma db pull && dotenv -e .env.prod.v3 -- npx prisma generate"
  },
